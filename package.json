{
  "name": "livekit-rnnoise-processor",
  "description": "A WebAssembly module implementing the RNNoise noise suppression library for livekit typescript.",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dadadah/livekit-rnnoise-processor.git"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:dockerimage": "docker image inspect -f '.' emscripten-autotools-4.0.4 || docker build -t emscripten-autotools-4.0.4 .",
    "build:emscripten": "docker run --rm -v $(pwd):/src emscripten-autotools-4.0.4 bash -x ./build.sh",
    "build:rnnoise": "pnpm run build:dockerimage && pnpm run build:emscripten",
    "build:worklet": "pnpm run build:rnnoise && webpack --config webpack.worklet.config.js"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "devDependencies": {
    "@types/audioworklet": "^0.0.56",
    "@types/compute-lcm": "^1.1.3",
    "@types/emscripten": "^1.39.13",
    "livekit-client": "^2.8.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.5.2",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1"
  },
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "compute-lcm": "^1.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
